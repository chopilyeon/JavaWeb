<style>
	div{
		width:100px;
		height:100px;
		background-color:red;
		}
</style>
<script src="js/jquery-3.6.0.min.js"></script>
<script>
$(document).ready(function(){ 
	$('div').animate({
		width:'200px',
		height:'200px',
		left : '100px', // 왜 안움직일까? 
		position:absolute;
				//position:'static' // position이 static임. 		
	},2000)
	//시작하는 위치도 변경할 수 있음. 
	
	
})
</script>







